#define _CRT_SECURE_NO_DEPRECATE
#define M_PI 3.14159265358979323846
#define ROW 9
#define COL 9
#include <stdio.h>
void main()
{
	int row, col;
	for (row = 1; row <= ROW; ++row)
	{
		for (col = 1; col <= COL; ++col)
		{
			printf("%d * %d = %d", row, col, row * col);
			printf("\t");
		}
		printf("\n");
	}

}
